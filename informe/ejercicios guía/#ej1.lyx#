\begin_body

\begin_layout Section*
Ejercicio 1
\end_layout

\begin_layout Standard
Analizar para los siguientes filtros invarianza en el tiempo, causalidad
 y linealidad.
\end_layout

\begin_layout Subsection*
Inciso d.
 
\begin_inset Formula $Y(nT)=5nT\,x^{2}(nT)$
\end_inset


\end_layout

\begin_layout Standard
Analizo invarianza:
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{1}(nT)\rightarrow y_{1}(nT)$
\end_inset

 con 
\begin_inset Formula $x_{1}(nT)=x(nT-\alpha T)$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $\Longrightarrow y_{1}(nT)=5nT\,x_{1}^{2}(nT)=5nT\,x^{2}(nT-\alpha T)\,\neq\,y(nT-\alpha T)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\shape italic
En conclusiÃ³n, al ser 
\begin_inset Formula $y_{1}(nT)\neq y(nT-\alpha T)$
\end_inset

 el sistema no es tiempo invariante.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Analizo causalidad:
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{1}(nT)=x_{2}(nT)\,\,\,\,\forall\,nT\leq kT$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{cases}
y_{1}(nT)=5nT\,x_{1}^{2}(kT)\\
y_{2}(nT)=5nT\,x_{2}^{2}(kT)
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\shape italic
Como 
\begin_inset Formula $kT\leq nT\Longrightarrow y_{1}(nT)=y_{2}(nT)$
\end_inset

 y el sistema es causal.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Analizo linealidad:
\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{cases}
x_{1}(nT)\rightarrow y_{1}(nT)\\
x_{2}(nT)\rightarrow y_{2}(nT)
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Longrightarrow\left[\alpha x_{1}+\beta x_{2}\right](nT)=5nT\alpha^{2}x_{1}^{2}(nT)+5nT\beta^{2}x_{2}^{2}(nT)\neq\alpha y_{1}(nT)+\beta y_{2}(nT)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\shape italic
Como 
\begin_inset Formula $\left[\alpha x_{1}+\beta x_{2}\right](nT)\neq\alpha y_{1}(nT)+\beta y_{2}(nT)$
\end_inset

 el sistema no es lineal.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Subsection*
Inciso e.
 
\begin_inset Formula $Y(nT)=3x(nT+3T)$
\end_inset


\end_layout

\begin_layout Standard
Analizo invarianza:
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{1}(nT)\rightarrow y_{1}(nT)$
\end_inset

 con 
\begin_inset Formula $x_{1}(nT)=x(nT-\alpha T)$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $\Longrightarrow y_{1}(nT)=3x_{1}(nT+3T)=3x(nT-\alpha T+3T)\,=\,y(nT-\alpha T)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\shape italic
Al ser 
\begin_inset Formula $y_{1}(nT)=y(nT-\alpha T)$
\end_inset

 el sistema es tiempo invariante.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Analizo causalidad:
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{1}(nT)=x_{2}(nT)\,\,\,\,\forall\,nT\leq kT$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{cases}
y_{1}(nT)=3x_{1}(kT+3T)\\
y_{2}(nT)=3x_{2}(kT+3T)
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\shape italic
Como 
\begin_inset Formula $kT+3T>nT\Longrightarrow y_{1}(nT)\neq y_{2}(nT)$
\end_inset

 y el sistema no es causal.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Analizo linealidad:
\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{cases}
x_{1}(nT)\rightarrow y_{1}(nT)\\
x_{2}(nT)\rightarrow y_{2}(nT)
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Longrightarrow\left[\alpha x_{1}+\beta x_{2}\right](nT)=3\alpha x_{1}(nT+3T)+3\beta x_{2}(nT+3T)=\alpha y_{1}(nT)+\beta y_{2}(nT)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\shape italic
Como 
\begin_inset Formula $\left[\alpha x_{1}+\beta x_{2}\right](nT)=\alpha y_{1}(nT)+\beta y_{2}(nT)$
\end_inset

 el sistema es lineal.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Subsection*
Inciso i.
 
\begin_inset Formula $Y(nT)=x(nT+T)e^{-nT}$
\end_inset


\end_layout

\begin_layout Standard
Analizo invarianza:
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{1}(nT)\rightarrow y_{1}(nT)$
\end_inset

 con 
\begin_inset Formula $x_{1}(nT)=x(nT-\alpha T)$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $\Longrightarrow y_{1}(nT)=x_{1}(nT+T)e^{-nT}=x(nT-\alpha T+T)e^{-nT}\,\neq\,y(nT-\alpha T)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\shape italic
Al ser 
\begin_inset Formula $y_{1}(nT)\neq y(nT-\alpha T)$
\end_inset

 el sistema no es tiempo invariante.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Analizo causalidad:
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{1}(nT)=x_{2}(nT)\,\,\,\,\forall\,nT\leq kT$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{cases}
y_{1}(nT)=x_{1}(kT+T)e^{-nT}\\
y_{2}(nT)=x_{1}(kT+T)e^{-nT}
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\shape italic
Como 
\begin_inset Formula $kT+T>nT\Longrightarrow y_{1}(nT)\neq y_{2}(nT)$
\end_inset

, por lo que el sistema no es causal.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Analizo linealidad:
\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{cases}
x_{1}(nT)\rightarrow y_{1}(nT)\\
x_{2}(nT)\rightarrow y_{2}(nT)
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Longrightarrow\left[\alpha x_{1}+\beta x_{2}\right](nT)=\alpha x_{1}(nT+T)e^{-nT}+\beta x_{1}(nT+T)e^{-nT}=\alpha y_{1}(nT)+\beta y_{2}(nT)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\shape italic
Como 
\begin_inset Formula $\left[\alpha x_{1}+\beta x_{2}\right](nT)=\alpha y_{1}(nT)+\beta y_{2}(nT)$
\end_inset

 el sistema es lineal.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{5mm}
\end_layout

\end_inset


\end_layout

